<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="AutomationTestSuite" parallel="tests" thread-count="2">
    <listeners>
        <listener class-name="io.qameta.allure.testng.AllureTestNg"/>
    </listeners>
    <test name="UI Tests">
        <classes>
            <class name="zm.co.gsb.tests.UITests"/>
        </classes>
    </test>
    <test name="API Tests">
        <classes>
            <class name="zm.co.gsb.tests.APITests"/>
        </classes>
    </test>
</suite>
